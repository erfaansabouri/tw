{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-intl": "*",
        "ariaieboy/filament-jalali-datetimepicker": "^2.0",
        "darkaonline/l5-swagger": "^8.5",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "guzzlehttp/guzzle": "^7.2",
        "hekmatinasser/verta": "^8.3",
        "jenssegers/agent": "^2.6",
        "kreait/laravel-firebase": "^5.6",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/telescope": "^4.14",
        "laravel/tinker": "^2.7",
        "maatwebsite/excel": "^3.1",
        "php-flasher/flasher-laravel": "^1.14",
        "php-flasher/flasher-toastr-laravel": "^1.14",
        "pxlrbt/filament-excel": "^2.3",
        "shetabit/payment": "^5.2",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-medialibrary": "^10.0.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "jubeki/laravel-code-style": "^2.4",
        "laravel/pint": "^1.10",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.1",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
